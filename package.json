  "author": "lemodragon",
  "license": "MIT",
  "picgo": {
    "name": "lskypro",
    "logo": "logo.png",
    "config": {
      "plugin.lskypro": {
        "required": true
      }
    }
  },
  "devDependencies": {
    "@types/node": "^10.10.1",
    "picgo": "^1.4.0",
    "tslint": "^5.10.0",
    "tslint-config-standard": "^7.1.0",
    "typescript": "^3.7.3",
    "eslint": "^5.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}