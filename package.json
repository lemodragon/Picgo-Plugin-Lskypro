{
  "name": "picgo-plugin-lskypro-own",
  "version": "1.0.0",
  "description": "PicGo 兰空图床(Lsky Pro)上传插件 - 支持 V1 和 V2 版本",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lemodragon/Picgo-Plugin-Lskypro"
  },
  "homepage": "https://github.com/lemodragon/Picgo-Plugin-Lskypro",
  "bugs": {
    "url": "https://github.com/lemodragon/Picgo-Plugin-Lskypro/issues"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc -p .",
    "dev": "tsc -w -p .",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "picgo",
    "picgo-plugin",
    "picgo-gui-plugin",
    "lsky-pro",
    "lskypro",
    "兰空图床",
    "image-uploader",
    "image-hosting"
